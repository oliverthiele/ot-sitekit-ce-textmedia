	"homepage": "https://www.oliver-thiele.de",
	"license": [
		"GPL-2.0-or-later"
	],
	"authors": [
		{
			"name": "Oliver Thiele",
			"email": "mail@oliver-thiele.de",
			"homepage": "https://www.oliver-thiele.de",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^13.4"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"conflict": {
	},
	"autoload": {
		"psr-4": {
			"OliverThiele\\OtSitekitcetextmedia\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "ot_sitekitcetextmedia"
		}
	}
